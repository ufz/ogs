                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0.0</t_initial>
                    <t_end>9e2</t_end>
                    <timesteps>
                        <pair>
                            <repeat>10</repeat>
                            <delta_t>1e-2</delta_t>
                        </pair>
                        <pair>
                            <repeat>40</repeat>
                            <delta_t>4e-2</delta_t>
                        </pair>
                        <pair>
                            <repeat>50</repeat>
                            <delta_t>8e-2</delta_t>
                        </pair>
                        <pair>
                            <repeat>50</repeat>
                            <delta_t>2e-1</delta_t>
                        </pair>
                        <pair>
                            <repeat>50</repeat>
                            <delta_t>4e-1</delta_t>
                        </pair>
