                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>0.0</t_initial>
                    <t_end>9e2</t_end>
                    <timesteps>
                        <pair>
                            <repeat>10</repeat>
                            <delta_t>1e-2</delta_t>
                        </pair>
                        <pair>
