                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>ThermalConvection_pcs_{:process_id}</prefix>
            <timesteps>
                <pair>
                    <repeat>21</repeat>
                    <each_steps>1</each_steps>
                </pair>
                <pair>
                    <repeat>1000</repeat>
                    <each_steps>50</each_steps>
                </pair>
            </timesteps>
            <output_iteration_results>false</output_iteration_results>
            <variables>
                <variable>T</variable>
                <variable>p</variable>
                <variable>darcy_velocity</variable>
            </variables>
            <suffix>_ts_{:timestep}_t_{:time}</suffix>
        </output>
    </time_loop>
    <parameters>
        <parameter>
            <name>rho_fluid</name>
            <type>Constant</type>
            <value>1000</value>
        </parameter>
        <parameter>
            <name>c_fluid</name>
            <type>Constant</type>
            <value>4200</value>
        </parameter>
        <parameter>
            <name>T0</name>
            <type>MeshNode</type>
            <field_name>initial_temperature</field_name>
        </parameter>
        <parameter>
            <name>P0</name>
            <type>MeshNode</type>
            <field_name>initial_pressure</field_name>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT0</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT1</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT2</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>p_Dirichlet_POINT3</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>t_Dirichlet_bottom</name>
            <type>Constant</type>
            <value>170</value>
        </parameter>
        <parameter>
            <name>t_Dirichlet_top</name>
            <type>Constant</type>
            <value>20</value>
        </parameter>
        <parameter>
            <name>kappa1</name>
            <type>Constant</type>
            <values>1.0e-18 0 0 0 1.0e-18 0 0 0 1.0e-18</values>
        </parameter>
        <parameter>
            <name>kappa2</name>
            <type>Constant</type>
            <values>1.7e-13 0 0 0 1.7e-13 0 0 0 1.7e-13</values>
        </parameter>
        <parameter>
            <name>constant_porosity_parameter</name>
            <type>Constant</type>
            <value>0.001</value>
        </parameter>
    </parameters>
    <process_variables>
        <process_variable>
            <name>T</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>T0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>Ra_795_fault_Top</mesh>
                    <type>Dirichlet</type>
                    <parameter>t_Dirichlet_top</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>Ra_795_fault_Bottom</mesh>
                    <type>Dirichlet</type>
                    <parameter>t_Dirichlet_bottom</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
        <process_variable>
            <name>p</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>P0</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <mesh>Ra_795_fault_POINT0</mesh>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT0</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>Ra_795_fault_POINT1</mesh>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT1</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>Ra_795_fault_POINT2</mesh>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT2</parameter>
                </boundary_condition>
                <boundary_condition>
                    <mesh>Ra_795_fault_POINT3</mesh>
                    <type>Dirichlet</type>
                    <parameter>p_Dirichlet_POINT3</parameter>
                </boundary_condition>
            </boundary_conditions>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_picard</name>
            <type>Picard</type>
            <max_iter>100</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <petsc>
                <prefix/>
                <parameters>-ksp_type bcgs -pc_type jacobi -ksp_norm_type unpreconditioned -ksp_rtol 1e-10 -ksp_atol 1e-10 -ksp_max_iter 30000 -ksp_monitor</parameters>
            </petsc>
            <lis>-i bicgstab -p jacobi -tol 1e-10 -maxiter 30000</lis>
            <eigen>
                <solver_type>BiCGSTAB</solver_type>
                <precon_type>DIAGONAL</precon_type>
                <max_iteration_step>30000</max_iteration_step>
                <error_tolerance>1e-10</error_tolerance>
            </eigen>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>
