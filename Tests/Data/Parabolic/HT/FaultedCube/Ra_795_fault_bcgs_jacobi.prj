                            <delta_t>4155</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>8260</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>16260</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>31580</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>60480</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>113000</delta_t>
                        </pair>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>205729</delta_t>
